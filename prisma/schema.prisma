 
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:postkarthik@localhost:5432/mydb?schema=sample"
}

model User {
  id           String @id
  email        String @unique
  display_name String @default("")
  about        String @default("")
  profileImg   String @default("")
}
